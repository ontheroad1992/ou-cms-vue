{
  "name": "ou-cms-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "mock": "nodemon ./mock",
    "start": "concurrently \"npm run mock\" \"npm run serve\""
  },
  "dependencies": {
    "@ant-design/icons": "^2.1.1",
    "ant-design-vue": "^1.4.12",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "lodash": "^4.17.21",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2",
    "vuex-persistedstate": "^2.7.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-plugin-router": "^4.2.3",
    "@vue/cli-plugin-vuex": "^4.2.3",
    "@vue/cli-service": "^4.2.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.13.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "koa": "^2.11.0",
    "koa-router": "^8.0.8",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mockjs": "^1.1.0",
    "nodemon": "^2.0.2",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "indent": [
        "warn",
        2
      ]
    }
  },
  "browserslist": [
    "cover 99.5%",
    "last 2 versions"
  ]
}
